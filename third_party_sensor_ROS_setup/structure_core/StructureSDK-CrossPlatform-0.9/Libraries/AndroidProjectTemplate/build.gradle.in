// Autogenerated, do not edit

buildscript {
    repositories {
        jcenter()
        google()
    }
    dependencies {
        classpath 'com.android.tools.build:gradle:3.1.4'
    }
}

apply plugin: 'com.android.application'

android {
    compileSdkVersion 24
    repositories {
        jcenter()
        google()
    }
    dependencies {
        implementation 'com.android.support:support-compat:24.2.1'
    }
    defaultConfig {
        applicationId = 'com.occipital.@CREATE_APP_NAME@'
        minSdkVersion 24
        targetSdkVersion 24
        versionCode = 1
        versionName = '1.0'
        externalNativeBuild {
            cmake {
                arguments '-DANDROID_STL=c++_shared'
                targets 'MainActivity'
                abiFilters 'arm64-v8a'
            }
        }
    }
    externalNativeBuild {
        cmake {
            path 'CMakeLists.txt'
        }
    }
    signingConfigs {
        config {
            storeFile file('sample.p12')
            storePassword 'sample'
            keyAlias 'sample'
            keyPassword 'sample'
            storeType 'pkcs12'
        }
    }
    buildTypes {
        debug {
            debuggable true
        }
        release {
            debuggable false
            minifyEnabled true
            signingConfig signingConfigs.config
        }
    }
    sourceSets {
        main {
            jniLibs.srcDir 'lib'
        }
    }
    lintOptions {
        checkReleaseBuilds false
    }
}
